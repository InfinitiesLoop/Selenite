{
  "Enabled": true,
  "DefaultDomain": "http://localhost/selenite/testpages/",
  "Tests": [
    {
      "Enabled": true,
      "Name": "DoCheckBox",
      "Url": "DoCommands.html",
      "Commands": [
        {
          "IsFalseExpected": true,
          "Selector": "input[name=\"CheckBoxA\"]",
          "Name": "IsChecked"
        },
        {
          "Selector": "input[name=\"CheckBoxA\"]",
          "Name": "DoCheckBox"
        },
        {
          "IsFalseExpected": false,
          "Selector": "input[name=\"CheckBoxA\"]",
          "Name": "IsChecked"
        }
      ]
    },
    {
      "Enabled": true,
      "Name": "DoClear",
      "Url": "DoCommands.html",
      "Commands": [
        {
          "Attribute": "value",
          "Value": "Hello World",
          "IsCaseSensitive": false,
          "IsFalseExpected": false,
          "Selector": "#DoClear",
          "Name": "IsAttributeEqual"
        },
        {
          "Selector": "#DoClear",
          "Name": "DoClear"
        },
        {
          "Attribute": "value",
          "Value": "",
          "IsCaseSensitive": false,
          "IsFalseExpected": false,
          "Selector": "#DoClear",
          "Name": "IsAttributeEqual"
        }
      ]
    },
    {
      "Enabled": true,
      "Name": "DoClick",
      "Url": "DoCommands.html",
      "Commands": [
        {
          "IsFalseExpected": true,
          "Selector": "#DoClick",
          "Name": "IsChecked"
        },
        {
          "Selector": "#DoClick",
          "Name": "DoClick"
        },
        {
          "IsFalseExpected": false,
          "Selector": "#DoClick",
          "Name": "IsChecked"
        }
      ]
    },
    {
